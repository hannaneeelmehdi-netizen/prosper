{
  "entities": {
    "contact": {
      "title": "Contact",
      "description": "A contact form submission from the website.",
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The sender's name."
        },
        "email": {
          "type": "string",
          "format": "email",
          "description": "The sender's email."
        },
        "message": {
          "type": "string",
          "description": "The message content."
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "description": "The timestamp when the message was sent."
        }
      },
      "required": ["name", "email", "message", "createdAt"]
    }
  },
  "auth": {
    "providers": []
  },
  "firestore": {
    "/contacts/{contactId}": {
      "schema": {
        "$ref": "#/entities/contact"
      },
      "description": "Stores all contact form submissions from the website."
    }
  }
}
